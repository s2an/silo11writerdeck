<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" 
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>io.silo11.writerdeck</string>

  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/open</string>
    <string>-a</string>
    <string>Terminal</string>
    <string>~/.local/bin/silo11writerdeck</string>
  </array>

  <key>RunAtLoad</key><true/>
  <key>KeepAlive</key><false/>
  <key>WorkingDirectory</key><string>%HOME%</string>
  <key>StandardOutPath</key><string>%HOME%/.local/state/silo11writerdeck/launchd.out.log</string>
  <key>StandardErrorPath</key><string>%HOME%/.local/state/silo11writerdeck/launchd.err.log</string>
</dict>
</plist>